---
import BlogPage from "../../components/blog/page.astro"
import Layout from '../../layouts/Layout.astro';

export const getStaticPaths = async () => {
  return [
    {
      params: { tag: "sine" }
    }
  ]
}
const { page } = Astro.params;
---
<Layout title="LiberBlog">
  <BlogPage page={parseInt(page? page:  "1")} />
</Layout>
